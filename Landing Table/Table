CREATE TABLE credit_raw (
    id TEXT,
    limit_bal TEXT,
    sex TEXT,
    education TEXT,
    marriage TEXT,
    age TEXT,

    pay_0 TEXT,
    pay_2 TEXT,
    pay_3 TEXT,
    pay_4 TEXT,
    pay_5 TEXT,
    pay_6 TEXT,

    bill_amt1 TEXT,
    bill_amt2 TEXT,
    bill_amt3 TEXT,
    bill_amt4 TEXT,
    bill_amt5 TEXT,
    bill_amt6 TEXT,

    pay_amt1 TEXT,
    pay_amt2 TEXT,
    pay_amt3 TEXT,
    pay_amt4 TEXT,
    pay_amt5 TEXT,
    pay_amt6 TEXT,

    default_payment_next_month TEXT
);

SELECT column_name
FROM information_schema.columns
WHERE table_name = 'credit_raw';
